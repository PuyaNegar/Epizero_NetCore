    }
    .financialSummaryPartial .modal-title {
        display: inline-block;
        float: right;
    }
    .financialSummaryPartial button.close {
        float: left;
    }
   
    .title-y {
        font-weight: 700;
        font-size: 14px;
        display: inline-block;
        width: 200px;
        text-align: right;
        margin-right: 3px;
