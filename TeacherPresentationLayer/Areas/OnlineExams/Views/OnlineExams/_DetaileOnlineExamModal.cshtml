<style type="text/css">
    .detail {
        margin-top:10px;
        margin-bottom : 0;
        padding : 20px;
    }
</style>


 <div class="modal fade" id="detailOnlineExamModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">جزئیات آزمون</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label for="Edit_Name">نام آزمون</label>
                            <input readonly="readonly" class="form-control" data-bind="value: detailOnlineExam().Name" type="text">
                         </div>
                    </div>
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label  for="Edit_StartDate">تاریخ شروع</label>
                            <input readonly="readonly" data-mask="9999/99/99" class="form-control valid" data-bind="value: detailOnlineExam().StartDate" type="text">
                         </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label  for="Edit_StartTime">ساعت شروع</label>
                            <input readonly="readonly" class="form-control clockpicker" readonly="readonly"    data-bind="value: detailOnlineExam().StartTime" type="text" >
                         </div>
                    </div>
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label for="Edit_Duration">مدت زمان آزمون (دقیقه)</label>
                            <input readonly="readonly" type="text" class="form-control" data-bind="value: detailOnlineExam().Duration">
                         </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label for="Edit_MaxGrade">نمره از چند</label>
                            <input readonly="readonly" type="text" class="form-control" data-bind="value: detailOnlineExam().MaxGrade">
                         </div>
                    </div>
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label for="Edit_AllowedTimeToEnter">محدودیت زمانی ورود به آزمون</label>
                            <input readonly="readonly" type="text" class="form-control" data-bind="value: detailOnlineExam().AllowedTimeToEnter" >
                         </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label for="Edit_QuestionTypeId">نوع سوالات آزمون </label>
                            <select disabled="disabled" class="form-control" data-bind="value: detailOnlineExam().QuestionTypeId" >
                                <option value="">لطفاٌ نوع سوالات  را انتخاب کنید ...</option>
                                <option value="2">تستی</option>
                            </select>
                         </div>
                    </div>
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label for="Edit_IsRandomQuestions">سوالات آزمون تصادفی باشد؟</label>
                            <select disabled="disabled" class="form-control" data-bind="value: detailOnlineExam().IsRandomQuestions">
                                <option value="1">بلی</option>
                                <option value="0">خیر</option>
                            </select>
                         </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6 pull-right">
                        <div class="form-group">
                            <label for="Edit_HasNegativePoint">آزمون نمره منفی دارد؟</label>
                            <select disabled="disabled" class="form-control" data-bind="value: detailOnlineExam().HasNegativePoint">
                                <option value="1">بلی</option>
                                <option value="0">خیر</option>
                            </select>
                         </div>
                    </div>
                    <div class="col-md-6 pull-right">
                        <div class="form-group random-div">
                            <label style="font-size:12px;" for="Edit_IsRandomOption">گزینه ها در سوالات برای دانشجویان تصادفی باشد؟</label>
                            <select disabled="disabled"  class="form-control" data-bind="value: detailOnlineExam().IsRandomOption">
                                <option selected="" value="0">خیر</option>
                            </select>
                         </div>
                    </div>

                </div>

            </div>
            <div>
                <hr />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger btn-sm" data-dismiss="modal">بستن</button>
            </div>
        </div>
    </div>
</div>
